import { createIframeWorkerMessenger } from './create-messenger.esnext';

function createRemoteOriginIframeMessengerFactory(iframeSrc) {
  return url => {
    return createIframeWorkerMessenger(url, iframe => {
      iframe.src = iframeSrc;
    });
  };
}

export { createRemoteOriginIframeMessengerFactory };
